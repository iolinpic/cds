{
  "env": {
    "es6": false,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:promise/recommended",
    "plugin:jest/all"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off"
  }
}